{
  "name": "pocket-api",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file .env  src/http/server.ts",
    "seed": "tsx --env-file .env src/db/seed.ts",
    "format": "biome format src --write",
    "pretest": "npm run db:migrate:test",
    "pretest:watch": "npm run db:migrate:test",
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit push:pg",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.4.0",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "3.0.3",
    "drizzle-kit": "^0.30.2",
    "exact": "^1.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/jwt": "^9.0.3",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "dayjs": "^1.11.13",
    "dotenv-cli": "^8.0.0",
    "drizzle-orm": "^0.38.4",
    "fastify": "^5.2.1",
    "fastify-type-provider-zod": "^4.0.2",
    "jose": "^5.9.6",
    "postgres": "^3.4.5"
  }
}